# Database
DATABASE_URL="postgresql://aip:aip_password@localhost:5432/aip_registry?schema=public"

# Server
PORT=3000
NODE_ENV=development

# CORS
CORS_ORIGIN=*

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info  # debug | info | warn | error

# Authentication (optional)
# Set to 'true' to require API keys for write operations
REQUIRE_API_KEY=false
